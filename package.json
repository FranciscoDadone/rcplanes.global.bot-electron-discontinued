{
  "name": "rcplanes.global.bot",
  "version": "1.0.0",
  "description": "Instagram bot to fetch and publish media.",
  "license": "MIT",
  "private": false,
  "repository": "https://github.com/FranciscoDadone/rcplanes.global.bot.git",
  "homepage": "",
  "bugs": {
    "url": "https://github.com/FranciscoDadone/rcplanes.global.bot/issues"
  },
  "author": "Francisco Dadone <dadonefran@gmail.com> (https://github.com/FranciscoDadone/)",
  "keywords": [
    "app",
    "bot",
    "instagram",
    "media"
  ],
  "engines": {
    "node": ">=9.0.0",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "browserslist": [
    "last 4 versions"
  ],
  "main": "main.js",
  "scripts": {
    "prod": "cross-env NODE_ENV=production webpack --mode production --config webpack.build.config.js && electron --noDevServer .",
    "start": "cross-env NODE_ENV=development webpack serve --hot --host 0.0.0.0 --config=./webpack.dev.config.js --mode development",
    "build": "cross-env NODE_ENV=production webpack --config webpack.build.config.js --mode production",
    "package": "npm run build",
    "postpackage": "electron-packager ./ --out=./builds"
  },
  "dependencies": {
    "bootstrap": "5.1.3",
    "download": "^8.0.0",
    "imgur": "^2.1.4",
    "instagram-web-api": "^2.2.2",
    "jimp": "^0.16.1",
    "jquery": "^3.6.0",
    "node-fetch": "2.0.0",
    "postcss": "^8.4.5",
    "react": "^17.0.2",
    "react-bootstrap": "^2.1.2",
    "react-dom": "^17.0.2",
    "sqlite3": "^5.0.2",
    "electron": "^16.0.5",
    "electron-packager": "^15.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "babel-loader": "^8.2.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.4.5",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^7.1.0",
    "postcss-pxtorem": "^6.0.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^3.11.3"
  }
